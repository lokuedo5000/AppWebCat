	<script type="text/javascript">
		$('.one_parte').show();
		$('.click_back').click(function(event) {
			var text = $('.num_back').text();
			var back_num = text.replace(/\*/g, "");
			$('.no_parte').fadeOut(200);
			if (back_num == '1') {
				$('.one_parte').fadeIn(200);
				$('.num_back').text('*1');
				$('.click_next').removeAttr('disabled');
				$('.click_back').attr('disabled', '');
				$('.click_save').hide();
			}
		});

		$('.click_next').click(function(event) {
			var text = $('.num_next').text();
			var next_num = text.replace(/\*/g, "");
			$('.no_parte').fadeOut(200);
			if (next_num == '1') {
				$('.two_parte').fadeIn(200);
				$('.num_next').text('*1');
				$('.click_back').removeAttr('disabled');
				$('.click_next').attr('disabled', '');
				$('.click_save').show();
			}

		});

		$('.click_save').click(function (event) {
			$('.no_parte').fadeOut(200);
			$('.save_id').click();
			$('.one_parte').fadeIn(200);
			$('.num_back').text('*1');
			$('.click_next').removeAttr('disabled');
			$('.click_back').attr('disabled', '');
			$('.click_save').hide();
		});

		function getgames() {
			var selectedValues = $('#loadedist').val();
			$('.getedit').attr('data-id', selectedValues);
			$('.save_id').attr('data-id', selectedValues);
			$('.getedit').click();
		}

		$('.clicktext').click(function(event) {
			alert($(this).text());
		});


	</script>

